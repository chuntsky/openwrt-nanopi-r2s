Subject: [PATCH] enable smartdns service by default

---
 package/openwrt/files/etc/config/smartdns | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

--- a/package/openwrt/files/etc/config/smartdns
+++ b/package/openwrt/files/etc/config/smartdns
@@ -1,5 +1,5 @@
 config 'smartdns'
-	option 'enabled' '0'
+	option 'enabled' '1'
 
 config server
 	option type 'udp'
